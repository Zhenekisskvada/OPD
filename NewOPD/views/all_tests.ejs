        ["voinarovsky_test","style_test_4.css"]
    ];
    let current = 0;
    let testList = testMatrix.map((arr) => arr[0]);

    function onLoad() {
        if (sessionStorage.getItem('currentTest') == null) {
            current = 0;
        } else {

            current = testList.indexOf(sessionStorage.getItem('currentTest'));
        }
        document.getElementById('stylesheet').href = "/public/css/"+testMatrix[current][1];
        document.getElementById('slidercss').href = "/public/libs/itc-slider/itc-slider.css";
        document.getElementById('testScript').src = '/public/libs/tests/'+testMatrix[current][0]+'.js';
        document.getElementById('current').innerHTML = document.getElementById(testMatrix[current][0]).innerHTML;
    }
    window.onload = onLoad;

    function nextTest() {
        current++;
        sessionStorage.setItem('currentTest', testList[current]);
        window.location.href = window.location.href; //DON'T YOU DARE REMOVE THIS
    }
</script>
